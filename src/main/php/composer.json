  "minimum-stability": "stable",
  "license": "GNU General Public License, version 2",
  "authors": [
    {
      "name": "metaVentis GmbH",
      "email": "support@edu-sharing.net"
    }
  ],
  "require": {
    "${composer.vendor}/community-repository-backend-services-rest-client-core": "${org.edu_sharing:edu_sharing-community-repository-backend-services-rest-client-php-core:zip.range}",
    "${composer.vendor}/enterprise-repository-plugin-transform-jodconverter-client-core": "${org.edu_sharing:edu_sharing-enterprise-repository-plugin-transform-jodconverter-client-php-core:zip.range}"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "${composer.repo.url}"
    }
  ]
